  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "compodoc": "compodoc",
    "generate-docs": "compodoc -p tsconfig.app.json",
