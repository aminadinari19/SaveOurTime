    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@types/jest": "^27.0.2",
    "ajv": "^6.9.1",
    "jest": "^27.3.1",
    "rxjs": "~6.6.0",
    "save-dev": "0.0.1-security",
    "ts-jest": "^27.0.7",
    "tslib": "^2.3.0",
    "uikit": "^3.8.0",
